{
  "expo": {
    "name": "WildWright",
    "slug": "wildwright",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "dark",
    "scheme": "wildwright",
    "splash": {
      "resizeMode": "contain",
      "backgroundColor": "#1f3527"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.phoenixgames.wildwright"
    },
    "android": {
      "package": "com.phoenixgames.wildwright"
    },
    "web": {
      "bundler": "metro"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
