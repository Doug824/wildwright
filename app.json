{
  "expo": {
    "name": "WildWright",
    "slug": "wildwright",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "dark",
    "scheme": "wildwright",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#1A3A2E"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.phoenixgames.wildwright",
      "icon": "./assets/icon.png"
    },
    "android": {
      "package": "com.phoenixgames.wildwright",
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon.png",
        "backgroundColor": "#1A3A2E"
      }
    },
    "web": {
      "bundler": "metro",
      "favicon": "./assets/icon.png",
      "themeColor": "#1A3A2E",
      "backgroundColor": "#1A3A2E",
      "display": "standalone",
      "orientation": "portrait",
      "startUrl": "/",
      "name": "WildWright",
      "shortName": "WildWright",
      "description": "Wild Shape companion app for Pathfinder druids",
      "lang": "en",
      "scope": "/",
      "icons": [
        {
          "src": "./assets/icon.png",
          "sizes": "any",
          "type": "image/png",
          "purpose": "any maskable"
        }
      ]
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
