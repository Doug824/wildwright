{
  "name": "wildwright",
  "version": "1.0.0",
  "main": "expo-router/entry",
  "scripts": {
    "start": "node node_modules/@expo/cli/build/bin/cli start",
    "android": "node node_modules/@expo/cli/build/bin/cli start --android",
    "ios": "node node_modules/@expo/cli/build/bin/cli start --ios",
    "web": "node node_modules/@expo/cli/build/bin/cli start --web",
    "build:web": "node node_modules/@expo/cli/build/bin/cli export --platform web --output-dir dist",
    "deploy": "npm run build:web && firebase deploy --only hosting",
    "seed-library": "tsx scripts/seedLibrary.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "^1.21.0",
    "@react-native-firebase/app": "^19.0.1",
    "@react-native-firebase/auth": "^19.0.1",
    "@react-native-firebase/firestore": "^19.0.1",
    "@tanstack/query-async-storage-persister": "^5.17.0",
    "@tanstack/react-query": "^5.17.0",
    "expo": "~54.0.0",
    "expo-haptics": "^15.0.7",
    "expo-linear-gradient": "^15.0.7",
    "expo-linking": "^8.0.8",
    "expo-router": "~4.0.0",
    "expo-status-bar": "~2.0.0",
    "firebase": "^10.7.1",
    "nativewind": "^4.0.0",
    "react": "18.3.1",
    "react-dom": "^18.3.1",
    "react-error-boundary": "^6.0.0",
    "react-native": "0.76.5",
    "react-native-safe-area-context": "~4.14.0",
    "react-native-screens": "~4.4.0",
    "react-native-web": "^0.19.13",
    "tailwindcss": "^3.4.0"
  },
  "devDependencies": {
    "@babel/core": "^7.24.0",
    "@types/node": "^24.9.1",
    "@types/react": "~18.2.45",
    "firebase-admin": "^13.5.0",
    "ts-node": "^10.9.2",
    "tsx": "^4.20.6",
    "typescript": "^5.3.0"
  },
  "private": true
}
